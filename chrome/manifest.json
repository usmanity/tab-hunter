{
    "manifest_version": 2,
    "name": "Tab Hunter",
    "version": "0.0.2",
    "content_scripts": [
        {
          "matches": ["http://*/*", "https://*/*"],
          "js": ["background.js"]
        }
    ],
    "commands": {
        "open-hunter": {
          "suggested_key": { "default": "Ctrl+K" },
          "description": "Toggle tab hunter"
        }
    }
}
